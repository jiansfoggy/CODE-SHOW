// a field of gray tone
// (also called a filled rectangle
//  but that is too long to type)
Command Field L B R T G
{
  Pen G
  Repeat H L R
  {
    Line H T H B
  }
}
Paper 0
// a test pattern
Repeat A 0 10
{
  Repeat B 0 10
  {
    Set A1 (A*10)
    Set B1 (B*10)
    Set A1 (A1+1)
    Set B1 (B1+1)
    Field A1 B1 (A1+3) (B1+3) (A*10)
    Set B1 (B1+5)
    Set A1 (A1+5)
    Field A1 B1 (A1+3) (B1+3) (B*10)
  }
}
